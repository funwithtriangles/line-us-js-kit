{
  "name": "webpack-boilerplate",
  "version": "4.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "start-page": "webpack-dev-server --progress --mode development --config webpack.config.js",
    "start-connection": "node lib/node-server",
    "start": "concurrently \"npm run start-connection\" \"npm run start-page\""
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "lodash": "4.17.10",
    "net": "^1.0.2",
    "socket.io": "^2.1.0",
    "standard": "^11.0.1"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.5",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.0",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2"
  }
}
